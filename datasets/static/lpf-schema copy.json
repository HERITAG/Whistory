{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "type",
    "@id",
    "properties",
    "names",
    "types",
    "geometry"
  ],
  "properties": {
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "Feature"
      ],
      "pattern": "^(.*)$"
    },
    "@id": {
      "$id": "#/properties/@id",
      "type": "string",
      "title": "The @id Schema",
      "default": "",
      "examples": [
        "https://www.hgis-indias.net/dokuwiki/doku.php?id=gazetteer:10000001"
      ],
      "pattern": "^(.*)$"
    },
    "properties": {
      "$id": "#/properties/properties",
      "type": "object",
      "title": "The Properties Schema",
      "required": [
        "title",
        "ccodes"
      ],
      "properties": {
        "title": {
          "$id": "#/properties/properties/properties/title",
          "type": "string",
          "title": "The Title Schema",
          "default": "",
          "examples": [
            "San Miguel"
          ],
          "pattern": "^(.*)$"
        },
        "ccodes": {
          "$id": "#/properties/properties/properties/ccodes",
          "type": "array",
          "title": "The Ccodes Schema",
          "items": {
            "$id": "#/properties/properties/properties/ccodes/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "CR"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "names": {
      "$id": "#/properties/names",
      "type": "array",
      "title": "The Names Schema",
      "items": {
        "$id": "#/properties/names/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "lang",
          "when",
          "toponym",
          "citation"
        ],
        "properties": {
          "lang": {
            "$id": "#/properties/names/items/properties/lang",
            "type": "string",
            "title": "The Lang Schema",
            "default": "",
            "examples": [
              "es"
            ],
            "pattern": "^(.*)$"
          },
          "when": {
            "$id": "#/properties/names/items/properties/when",
            "type": "object",
            "title": "The When Schema",
            "required": [
              "timespans"
            ],
            "properties": {
              "timespans": {
                "$id": "#/properties/names/items/properties/when/properties/timespans",
                "type": "array",
                "title": "The Timespans Schema",
                "items": {
                  "$id": "#/properties/names/items/properties/when/properties/timespans/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "end",
                    "start"
                  ],
                  "properties": {
                    "end": {
                      "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/end",
                      "type": "string",
                      "title": "The End Schema",
                      "default": "",
                      "examples": [
                        "1708"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "start": {
                      "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/start",
                      "type": "string",
                      "title": "The Start Schema",
                      "default": "",
                      "examples": [
                        "1708"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          },
          "toponym": {
            "$id": "#/properties/names/items/properties/toponym",
            "type": "string",
            "title": "The Toponym Schema",
            "default": "",
            "examples": [
              "San Miguel"
            ],
            "pattern": "^(.*)$"
          },
          "citation": {
            "$id": "#/properties/names/items/properties/citation",
            "type": "object",
            "title": "The Citation Schema",
            "required": [
              "@id",
              "label"
            ],
            "properties": {
              "@id": {
                "$id": "#/properties/names/items/properties/citation/properties/@id",
                "type": "null",
                "title": "The @id Schema",
                "default": null,
                "examples": [
                  null
                ]
              },
              "label": {
                "$id": "#/properties/names/items/properties/citation/properties/label",
                "type": "string",
                "title": "The Label Schema",
                "default": "",
                "examples": [
                  "some text"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    },
    "types": {
      "$id": "#/properties/types",
      "type": "array",
      "title": "The Types Schema",
      "items": {
        "$id": "#/properties/types/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "label",
          "identifier",
          "sourceLabel"
        ],
        "properties": {
          "label": {
            "$id": "#/properties/types/items/properties/label",
            "type": "string",
            "title": "The Label Schema",
            "default": "",
            "examples": [
              "village"
            ],
            "pattern": "^(.*)$"
          },
          "identifier": {
            "$id": "#/properties/types/items/properties/identifier",
            "type": "string",
            "title": "The Identifier Schema",
            "default": "",
            "examples": [
              "aat:300008372"
            ],
            "pattern": "^(.*)$"
          },
          "sourceLabel": {
            "$id": "#/properties/types/items/properties/sourceLabel",
            "type": "string",
            "title": "The Sourcelabel Schema",
            "default": "",
            "examples": [
              "Pueblo"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "geometry": {
      "$id": "#/properties/geometry",
      "type": "object",
      "title": "The Geometry Schema",
      "required": [
        "type",
        "geometries"
      ],
      "properties": {
        "type": {
          "$id": "#/properties/geometry/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "GeometryCollection"
          ],
          "pattern": "^(.*)$"
        },
        "geometries": {
          "$id": "#/properties/geometry/properties/geometries",
          "type": "object",
          "title": "The Geometries Schema",
          "required": [
            "type",
            "when",
            "geowkt",
            "coordinates"
          ],
          "properties": {
            "type": {
              "$id": "#/properties/geometry/properties/geometries/properties/type",
              "type": "string",
              "title": "The Type Schema",
              "default": "",
              "examples": [
                "Point"
              ],
              "pattern": "^(.*)$"
            },
            "when": {
              "$id": "#/properties/geometry/properties/geometries/properties/when",
              "type": "object",
              "title": "The When Schema",
              "required": [
                "timespans"
              ],
              "properties": {
                "timespans": {
                  "$id": "#/properties/geometry/properties/geometries/properties/when/properties/timespans",
                  "type": "array",
                  "title": "The Timespans Schema",
                  "items": {
                    "$id": "#/properties/geometry/properties/geometries/properties/when/properties/timespans/items",
                    "type": "object",
                    "title": "The Items Schema",
                    "required": [
                      "end",
                      "start"
                    ],
                    "properties": {
                      "end": {
                        "$id": "#/properties/geometry/properties/geometries/properties/when/properties/timespans/items/properties/end",
                        "type": "object",
                        "title": "The End Schema",
                        "required": [
                          "in"
                        ],
                        "properties": {
                          "in": {
                            "$id": "#/properties/geometry/properties/geometries/properties/when/properties/timespans/items/properties/end/properties/in",
                            "type": "string",
                            "title": "The In Schema",
                            "default": "",
                            "examples": [
                              "1708"
                            ],
                            "pattern": "^(.*)$"
                          }
                        }
                      },
                      "start": {
                        "$id": "#/properties/geometry/properties/geometries/properties/when/properties/timespans/items/properties/start",
                        "type": "object",
                        "title": "The Start Schema",
                        "required": [
                          "in"
                        ],
                        "properties": {
                          "in": {
                            "$id": "#/properties/geometry/properties/geometries/properties/when/properties/timespans/items/properties/start/properties/in",
                            "type": "string",
                            "title": "The In Schema",
                            "default": "",
                            "examples": [
                              "1708"
                            ],
                            "pattern": "^(.*)$"
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "geowkt": {
              "$id": "#/properties/geometry/properties/geometries/properties/geowkt",
              "type": "string",
              "title": "The Geowkt Schema",
              "default": "",
              "examples": [
                "POINT(-83.117154 9.539739)"
              ],
              "pattern": "^(.*)$"
            },
            "coordinates": {
              "$id": "#/properties/geometry/properties/geometries/properties/coordinates",
              "type": "array",
              "title": "The Coordinates Schema",
              "items": {
                "$id": "#/properties/geometry/properties/geometries/properties/coordinates/items",
                "type": "number",
                "title": "The Items Schema",
                "default": 0.0,
                "examples": [
                  -83.117154,
                  9.539739
                ]
              }
            }
          }
        }
      }
    }
  }
}
