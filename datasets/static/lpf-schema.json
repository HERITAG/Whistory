{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "type",
    "@id",
    "properties",
    "names"
  ],
  "properties": {
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "enum": ["Feature"]
    },
    "@id": {
      "$id": "#/properties/@id",
      "type": "string",
      "title": "The @id Schema",
      "default": "",
      "examples": [
        "https://foofah.com"
      ],
      "pattern": "^(.*)$"
    },
    "properties": {
      "$id": "#/properties/properties",
      "type": "object",
      "title": "The Properties Schema",
      "required": ["title","ccodes"],
      "properties": {
        "title": {
          "$id": "#/properties/properties/properties/title",
          "type": "string",
          "title": "The Title Schema",
          "default": "",
          "examples": ["San Miguel"],
          "pattern": "^(.*)$"
        },
        "ccodes": {
          "$id": "#/properties/properties/properties/ccodes",
          "type": "array",
          "title": "The Ccodes Schema",
          "items": {
            "$id": "#/properties/properties/properties/ccodes/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": ["CR"],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "names": {
      "$id": "#/properties/names",
      "type": "array",
      "title": "The Names Schema",
      "items": {
        "$id": "#/properties/names/items",
        "type": "object",
        "title": "The Items Schema",
        "required": ["lang","toponym","citation"],
        "properties": {
          "lang": {
            "$id": "#/properties/names/items/properties/lang",
            "type": "string",
            "title": "The Lang Schema",
            "examples": ["es"],
            "pattern": "^(.*)$"
          },
          "when": {
            "$id": "#/properties/names/items/properties/when",
            "type": "object",
            "title": "The Name When Schema",
            "properties": {
              "timespans": {
                "$id": "#/properties/names/items/properties/when/properties/timespans",
                "type": "array",
                "title": "The Timespans Schema",
                "items": {
                  "$id": "#/properties/names/items/properties/when/properties/timespans/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": ["start"],
                  "properties": {
                    "end": {
                      "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/end",
                      "type": "object",
                      "title": "The End Schema",
                      "properties": {
                        "in": {"type": "string"}
                      },
                      "required": ["in"]
                    },
                    "start": {
                      "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/start",
                      "type": "object",
                      "title": "The Start Schema",
                      "properties": {
                        "in": {"type": "string"}
                      },
                      "required": ["in"]
                    }
                  }
                }
              },
              "periods": {
                "$id": "#/properties/names/items/properties/when/properties/periods",
                "type": "array",
                "items": {
                  "$id": "#/properties/names/properties/periods/items",
                  "type": "object",
                  "title": "The Periods Schema",
                  "properties": {
                    "name": {"type": "string"},
                    "uri": {"type": "string"}
                  },
                  "required": ["name"]
                }
              },
              "label": {"type": "string"},
              "duration": {"type": "string"}
            }
          },
          "toponym": {
            "$id": "#/properties/names/items/properties/toponym",
            "type": "string",
            "title": "The Toponym Schema",
            "examples": ["San Miguel"],
            "pattern": "^(.*)$"
          },
          "citation": {
            "$id": "#/properties/names/items/properties/citation",
            "type": "object",
            "title": "The Citation Schema",
            "required": [],
            "properties": {
              "@id": {
                "$id": "#/properties/names/items/properties/citation/properties/@id",
                "type": "string",
                "title": "The @id Schema",
                "examples": ["http://foofah.org"]
              },
              "label": {
                "$id": "#/properties/names/items/properties/citation/properties/label",
                "type": "string",
                "title": "The Label Schema",
                "examples": ["Blow, J. (1708)"],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    },
    "types": {
      "$id": "#/properties/types",
      "type": "array",
      "title": "The Types Schema",
      "items": {
        "$id": "#/properties/types/items",
        "type": "object",
        "title": "The Items Schema",
        "properties": {
          "label": {
            "$id": "#/properties/types/items/properties/label",
            "type": "string",
            "title": "The Label Schema",
            "default": "",
            "examples": ["village"],
            "pattern": "^(.*)$"
          },
          "identifier": {
            "$id": "#/properties/types/items/properties/identifier",
            "type": "string",
            "title": "The Identifier Schema",
            "default": "",
            "examples": ["aat:300008372"],
            "pattern": "^(.*)$"
          },
          "sourceLabel": {
            "$id": "#/properties/types/items/properties/sourceLabel",
            "type": "string",
            "title": "The Sourcelabel Schema",
            "default": "",
            "examples": ["Pueblo"],
            "pattern": "^(.*)$"
          },
          "when": {
            "$id": "#/properties/names/items/properties/when",
            "type": "object",
            "title": "The Name When Schema",
            "properties": {
              "timespans": {
                "$id": "#/properties/names/items/properties/when/properties/timespans",
                "type": "array",
                "title": "The Timespans Schema",
                "items": {
                  "$id": "#/properties/names/items/properties/when/properties/timespans/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": ["start"],
                  "properties": {
                    "end": {
                      "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/end",
                      "type": "object",
                      "title": "The End Schema",
                      "properties": {
                        "in": {"type": "string"}
                      },
                      "required": ["in"]
                    },
                    "start": {
                      "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/start",
                      "type": "object",
                      "title": "The Start Schema",
                      "properties": {
                        "in": {"type": "string"}
                      },
                      "required": ["in"]
                    }
                  }
                }
              },
              "periods": {
                "$id": "#/properties/names/items/properties/when/properties/periods",
                "type": "array",
                "items": {
                  "$id": "#/properties/names/properties/periods/items",
                  "type": "object",
                  "title": "The Periods Schema",
                  "properties": {
                    "name": {"type": "string"},
                    "uri": {"type": "string"}
                  },
                  "required": ["name"]
                }
              },
              "label": {"type": "string"},
              "duration": {"type": "string"}
            }
          }
        }
      }
    },
    "geometry": {
      "$id": "#/properties/geometry",
      "type": "object",
      "title": "The Geometry Schema",
      "required": [
        "type",
        "geometries"
      ],
      "properties": {
        "type": {
          "$id": "#/properties/geometry/properties/type",
          "type": "string",
          "enum": ["GeometryCollection"],
          "title": "The Type Schema",
          "pattern": "^(.*)$"
        },
        "geometries": {
          "$id": "#/properties/geometry/properties/geometries",
          "type": "array",
          "title": "The Geometries Schema",
          "items": {
            "$id": "#/properties/geometry/properties/geometries/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "type",
              "coordinates"
            ],
            "properties": {
              "type": {
                "$id": "#/properties/geometry/properties/geometries/items/properties/type",
                "type": "string",
                "title": "The Type Schema",
                "pattern": "^(.*)$"
              },
              "coordinates": {
                "$id": "#/properties/geometry/properties/geometries/items/properties/coordinates",
                "type": "array",
                "title": "The Coordinates Schema",
                "items": {
                  "$id": "#/properties/geometry/properties/geometries/items/properties/coordinates/items",
                  "type": "number",
                  "title": "The Items Schema"
                }
              },
              "when": {
                "$id": "#/properties/names/items/properties/when",
                "type": "object",
                "title": "The Name When Schema",
                "properties": {
                  "timespans": {
                    "$id": "#/properties/names/items/properties/when/properties/timespans",
                    "type": "array",
                    "title": "The Timespans Schema",
                    "items": {
                      "$id": "#/properties/names/items/properties/when/properties/timespans/items",
                      "type": "object",
                      "title": "The Items Schema",
                      "required": ["start"],
                      "properties": {
                        "end": {
                          "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/end",
                          "type": "object",
                          "title": "The End Schema",
                          "properties": {
                            "in": {"type": "string"}
                          },
                          "required": ["in"]
                        },
                        "start": {
                          "$id": "#/properties/names/items/properties/when/properties/timespans/items/properties/start",
                          "type": "object",
                          "title": "The Start Schema",
                          "properties": {
                            "in": {"type": "string"}
                          },
                          "required": ["in"]
                        }
                      }
                    }
                  },
                  "periods": {
                    "$id": "#/properties/names/items/properties/when/properties/periods",
                    "type": "array",
                    "items": {
                      "$id": "#/properties/names/properties/periods/items",
                      "type": "object",
                      "title": "The Periods Schema",
                      "properties": {
                        "name": {"type": "string"},
                        "uri": {"type": "string"}
                      },
                      "required": ["name"]
                    }
                  },
                  "label": {"type": "string"},
                  "duration": {"type": "string"}
                }
              },
              "geowkt": {
                "$id": "#/properties/geometry/properties/geometries/items/properties/geowkt",
                "type": "string",
                "title": "The Geowkt Schema",
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}
